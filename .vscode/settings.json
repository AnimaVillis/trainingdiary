{
  "files.exclude": {
    // A list of files to exclude from file explorer and search results.
    "node_modules/": true,
    "package-lock.json": true,
     "composer.lock": true,
    // "Dockerfile": true,
    // ".github": false,
    ".git": true,
    // ".gitignore": false,
    // "captain-definition": true,
    // "vite.config.js": true,
    "backend/vendor": true,
    "*/dist": true,
    // "package.json": false,
    // "composer.json": false,
    // "ts*.json": true,
    "tmp/*": true,
  },
  "vue3snippets.enable-compile-vue-file-on-did-save-code": true,
  "cSpell.diagnosticLevel": "Hint",
  "scss.lint.unknownAtRules": "ignore",
  "recommendations": ["Vue.volar", "Vue.vscode-typescript-vue-plugin"]
}
